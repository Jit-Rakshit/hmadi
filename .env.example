# EmailJS Configuration
# Get these credentials from https://www.emailjs.com/

# EmailJS Service ID (from Email Services section)
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# EmailJS Template ID for customer confirmation email
VITE_EMAILJS_CUSTOMER_TEMPLATE_ID=your_customer_template_id_here

# EmailJS Template ID for owner notification email
VITE_EMAILJS_OWNER_TEMPLATE_ID=your_owner_template_id_here

# EmailJS Public Key (from Account > API Keys)
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Salon owner email address (where booking notifications will be sent)
VITE_OWNER_EMAIL=hello@hmadi.salon
